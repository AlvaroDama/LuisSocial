<?xml version="1.0" encoding="utf-8" ?>

<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LuisSocial.View.Mensajes.DetalleMensaje"

             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             xmlns:viewModel1="clr-namespace:LuisSocial.ViewModel.Mensajes;assembly=LuisSocial"
             d:DataContext="{d:DesignInstance viewModel1:DetalleMensajeViewModel}"

             Title="{Binding Titulo}">

  
  <StackLayout>
    <Label Text="{Binding Mensaje.IdOrigen}"></Label>
    <Label Text="{Binding Mensaje.Asunto}"></Label>
    <Label Text="{Binding Mensaje.Contenido}"></Label>
  </StackLayout>
</ContentPage>